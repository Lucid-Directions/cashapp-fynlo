# Test Environment Configuration
# Uses real services in test mode

# Application
APP_NAME=Fynlo POS Test
DEBUG=True
ENVIRONMENT=test
API_V1_STR=/api/v1
BASE_URL=http://localhost:8000

# Database - Real PostgreSQL test database
DATABASE_URL=postgresql://fynlo_test:fynlo_test_password@localhost:5432/fynlo_pos_test

# Redis - Test database (use different DB number)
REDIS_URL=redis://localhost:6379/15

# Security
SECRET_KEY=test-secret-key-for-testing-only
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Supabase - Test project (you'll need to set these)
SUPABASE_URL=${SUPABASE_TEST_URL}
SUPABASE_ANON_KEY=${SUPABASE_TEST_ANON_KEY}
SUPABASE_SERVICE_ROLE_KEY=${SUPABASE_TEST_SERVICE_ROLE_KEY}
PLATFORM_OWNER_EMAIL=test_admin@fynlo.co.uk
PLATFORM_OWNER_SECRET_KEY=test-platform-secret

# Payment Processing - Test/Sandbox modes
STRIPE_SECRET_KEY=${STRIPE_TEST_SECRET_KEY}
STRIPE_PUBLISHABLE_KEY=${STRIPE_TEST_PUBLISHABLE_KEY}
STRIPE_WEBHOOK_SECRET=${STRIPE_TEST_WEBHOOK_SECRET}

# Square - Sandbox
SQUARE_APPLICATION_ID=${SQUARE_SANDBOX_APPLICATION_ID}
SQUARE_ACCESS_TOKEN=${SQUARE_SANDBOX_ACCESS_TOKEN}
SQUARE_LOCATION_ID=${SQUARE_SANDBOX_LOCATION_ID}
SQUARE_WEBHOOK_SIGNATURE_KEY=${SQUARE_SANDBOX_WEBHOOK_SIGNATURE_KEY}
SQUARE_ENVIRONMENT=sandbox

# SumUp - Sandbox
SUMUP_API_KEY=${SUMUP_SANDBOX_API_KEY}
SUMUP_MERCHANT_CODE=${SUMUP_SANDBOX_MERCHANT_CODE}
SUMUP_AFFILIATE_KEY=${SUMUP_SANDBOX_AFFILIATE_KEY}
SUMUP_ENVIRONMENT=sandbox

# QR Payment Settings
QR_PAYMENT_FEE_PERCENTAGE=1.2
DEFAULT_CARD_FEE_PERCENTAGE=2.9

# WebSocket
WEBSOCKET_HOST=localhost
WEBSOCKET_PORT=8001

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=test_uploads
ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,pdf,docx,xlsx

# DigitalOcean Spaces - Test bucket
SPACES_ACCESS_KEY_ID=${SPACES_TEST_ACCESS_KEY_ID}
SPACES_SECRET_ACCESS_KEY=${SPACES_TEST_SECRET_ACCESS_KEY}
SPACES_BUCKET=fynlo-pos-test-storage
SPACES_REGION=lon1
SPACES_ENDPOINT=https://lon1.digitaloceanspaces.com
CDN_ENDPOINT=
ENABLE_SPACES_STORAGE=False

# Email Service - Test mode
RESEND_API_KEY=${RESEND_TEST_API_KEY}
RESEND_FROM_EMAIL=test@fynlo.co.uk
RESEND_FROM_NAME=Fynlo POS Test

# Logging
LOG_LEVEL=DEBUG
ERROR_DETAIL_ENABLED=True