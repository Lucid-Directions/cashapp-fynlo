# Example Environment Configuration
# Copy this file to .env and fill in your values
# DO NOT commit the actual .env file

# API Configuration
API_BASE_URL=https://api.yourdomain.com
WEBSOCKET_URL=wss://api.yourdomain.com/ws

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=pk_live_your_key_here
STRIPE_MERCHANT_ID=merchant.com.yourcompany.payments

# SumUp Configuration
SUMUP_API_KEY=your_sumup_api_key_here
SUMUP_MERCHANT_CODE=your_merchant_code_here

# Sentry Configuration
SENTRY_DSN=https://your_sentry_dsn@sentry.io/your_project_id

# Environment
NODE_ENV=production
ENABLE_DEBUG_LOGGING=false